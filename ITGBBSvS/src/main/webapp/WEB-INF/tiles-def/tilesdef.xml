			<add-attribute value="/scripts/itg.rank.js" />
			<add-attribute value="http://code.jquery.com/jquery-1.12.4.js" />
		</put-list-attribute>
		<put-list-attribute name="stylesheets">
			<add-attribute value="/styles/rank.css" />
		</put-list-attribute>
	</definition>

	<definition name="rankList" extends="boardList">
